<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="menuPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unibo.pyxis.view.views.MenuSceneView">
   <children>
      <Button fx:id="newGameButton" layoutX="70.0" layoutY="250.0" mnemonicParsing="false" onAction="#startNewGame" prefHeight="26.0" prefWidth="191.0" text="New Game" textFill="#1a00ff" />
      <Button fx:id="settingsButton" layoutX="70.0" layoutY="318.0" mnemonicParsing="false" onAction="#showSettings" prefHeight="26.0" prefWidth="191.0" text="Settings" textFill="#1a00ff" />
      <Button fx:id="levelsButton" layoutX="339.0" layoutY="250.0" mnemonicParsing="false" onAction="#selectLevels" prefHeight="26.0" prefWidth="191.0" text="Levels" textFill="#1a00ff" />
      <Button fx:id="quitButton" layoutX="339.0" layoutY="318.0" mnemonicParsing="false" onAction="#quit" prefHeight="26.0" prefWidth="191.0" text="Quit" textFill="#1a00ff" />
      <ImageView fx:id="menuImageView" fitHeight="103.0" fitWidth="191.0" layoutX="209.0" layoutY="44.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
